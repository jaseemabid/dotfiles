# thumb wheel up => increase volume and unmute
sh -c "pactl set-sink-mute @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ +5%"
   b:7

# thumb wheel down => lower volume
sh -c "pactl set-sink-mute  @DEFAULT_SINK@ false ; pactl set-sink-volume @DEFAULT_SINK@ -5%"
   b:6

"playerctl next"
   XF86AudioNext

"playerctl previous"
   XF86AudioPrev

"playerctl play"
    XF86AudioPlay

"playerctl pause"
    XF86AudioPause
